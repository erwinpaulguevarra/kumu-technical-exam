{
    "openapi": "3.0.1",
    "info": {
        "title": "Kumu Technical Exam API",
        "description": "Create PHP API project that has an API endpoint that takes a list of\ngithub usernames (up to a maximum of 10 names) and returns to the user a list\nof basic information.",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://kumu-technical-exam.herokuapp.com/api/v1"
        }
    ],
    "paths": {
        "/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register",
                "operationId": "createUser",
                "requestBody": {
                    "content": {
                        "*/*": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RegisterAndLoginResponse"
                                }
                            }
                        }
                    }
                },
                "x-codegen-request-body-name": "body"
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login",
                "operationId": "",
                "requestBody": {
                    "content": {
                        "*/*": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RegisterAndLoginResponse"
                                }
                            }
                        }
                    }
                },
                "x-codegen-request-body-name": "body"
            }
        },
        "/git/users": {
            "get": {
                "tags": [
                    "Git"
                ],
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "summary": "Get the information of multiple user.",
                "operationId": "",
                "parameters": [
                    {
                        "name": "usernames[]",
                        "in": "query",
                        "required": true,
                        "style": "form",
                        "explode": true,
                        "schema": {
                            "maxItems": 10,
                            "minItems": 1,
                            "uniqueItems": true,
                            "type": "array"
                        },
                        "example": '{ "usernames": [ "user1", "user2", "user3", "user4", "user5" ] }'
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GitUsersResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "RegisterRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "xml": {
                    "name": "Register"
                }
            },
            "LoginRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "xml": {
                    "name": "LoginRequest"
                }
            },
            "GitUsersRequest": {
                "type": "object",
                "properties": {
                    "usernames": {
                        "type": "object",
                        "properties": {},
                        "example": [
                            "user1",
                            "user2",
                            "user3"
                        ]
                    }
                },
                "xml": {
                    "name": "LoginRequest"
                }
            },
            "RegisterAndLoginResponse": {
                "type": "object",
                "properties": {
                    "access_token": {
                        "type": "string",
                        "example": "2|EGul2xvXjY5830MbdgdfDw0TUaseL3HGsFeMTiHh"
                    },
                    "token_type": {
                        "type": "string",
                        "example": "Bearer"
                    }
                }
            },
            "GitUsersResponse": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "object",
                        "properties": {},
                        "example": [
                            {
                                "name": null,
                                "login": "user1",
                                "company": null,
                                "followers": 1,
                                "public_repositories": 1,
                                "avg_followers_per_repo": 1
                            },
                            {
                                "name": null,
                                "login": "user2",
                                "company": null,
                                "followers": 0,
                                "public_repositories": 0,
                                "avg_followers_per_repo": 0
                            },
                            {
                                "name": null,
                                "login": "user3",
                                "company": null,
                                "followers": 1,
                                "public_repositories": 0,
                                "avg_followers_per_repo": 0
                            }
                        ]
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    }
}
